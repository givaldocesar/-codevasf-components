  "version": "1.1.0",
  "description": "Componentes React para desenvolvimento de projetos WEB da CODEVASF",
  "files": ["dist"],
  "exports": {
    ".": {
      "types": "./dist/esm/index.d.ts",
      "import": "./dist/esm/index.es.js",
      "require": "./dist/cjs/index.js"
    },
    "./styles": {
      "import": "./dist/esm/styles.css",
      "require": "./dist/cjs/styles.css"
    },
    "./cards": {
      "types": "./dist/esm/cards/index.d.js",
      "import": "./dist/esm/cards/index.es.js",
      "require": "./dist/cjs/cards/index.js"
    },
    "./header": { 
      "types": "./dist/esm/header/index.d.js",
      "import": "./dist/esm/header/index.es.js",
      "require": "./dist/cjs/header/index.js"
    }
  },
  "scripts": {
    "build": "rollup -c",
    "prepublish": "build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/givaldocesar/codevasf-components.git"
  },
  "keywords": [
    "react",
    "typescript",
    "codevasf"
  ],
  "author": {
