  "homepage": "https://github.com/givaldocesar/codevasf-components#readme",
  "peerDependencies": {
    "classnames": "^2.5.1",
    "next": "^14.2.5",
    "react": "^18",
    "react-dom": "^18",
    "sass": "^1.77.8"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^26.0.1",
